{
  "name": "bs-msw",
  "version": "0.2.0",
  "description": "Mock Service Worker (MSW) bindings for BuckleScript in Reason",
  "repository": "https://github.com/jihchi/bs-msw.git",
  "homepage": "https://github.com/jihchi/bs-msw",
  "bugs": {
    "url": "https://github.com/jihchi/bs-msw/issues"
  },
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "dev-server": "webpack-dev-server",
    "format": "find ./src ./__tests__ -regex '.*\\.rei\\{0,1\\}' -exec bsrefmt --in-place '{}' \\;",
    "prepublishOnly": "npm test",
    "start": "npm run build -- -w",
    "test": "jest",
    "coverage": "BISECT_ENABLE=yes npm run build && npm test",
    "cleanbuild": "npm run clean && npm run build",
    "cleanbisect": "rm bisect*.coverage || true",
    "cleancoverage": "npm run cleanbisect && npm run clean && npm run coverage",
    "cleantest": "npm run cleanbuild && npm test"
  },
  "keywords": [
    "BuckleScript",
    "ReasonML",
    "bindings",
    "mock",
    "mocking",
    "msw"
  ],
  "author": "Jihchi Lee <achi@987.tw>",
  "license": "MIT",
  "files": [
    "/src/**/*.re",
    "bsconfig.json"
  ],
  "dependencies": {
    "bisect_ppx": "^2.4.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-fetch": "^0.6.1",
    "bs-platform": "^7.2.2",
    "jest": "^26.0.1",
    "jest-puppeteer": "^4.4.0",
    "msw": "^0.19.3",
    "node-fetch": "^2.6.0",
    "puppeteer": "^4.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "msw": "^0.19.0"
  }
}
