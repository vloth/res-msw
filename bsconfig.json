{
  "name": "bs-msw",
  "version": "0.1.0",
  "namespace": true,
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "sources": [
    { "dir": "src" },
    { "dir": "__tests__", "type": "dev", "subdirs": ["support"] }
  ],
  "bs-dependencies": ["bisect_ppx"],
  "suffix": ".bs.js",
  "bs-dev-dependencies": ["@glennsl/bs-jest", "bs-fetch"],
  "ppx-flags": [
    ["bisect_ppx/ppx", "--exclude-files", ".*_test\\.re$$", "--exclude-files", ".*\\/__tests__\\/support\\/.*\\.re$$"]
  ],
  "warnings": {
    "error": "+101"
  },
  "bsc-flags": ["-open Belt"],
  "refmt": 3
}
